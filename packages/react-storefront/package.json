{
  "name": "react-storefront",
  "version": "6.81.1",
  "description": "Build and deploy e-commerce progressive web apps in record time.",
  "browser": "src",
  "module": "./index.js",
  "author": "Moovweb",
  "license": "Apache-2.0",
  "scripts": {
    "test:unit": "jest --runInBand --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!test/**/*",
    "test": "yarn test:unit && yarn test:browser",
    "test:browser": "webpack --config=config/webpack.config.js && karma start config/karma.conf.js --single-run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "create-indexes": "create-index -ri --banner \"/**\n * @license\n * Copyright Â© 2017-2018 Moov Corporation.  All rights reserved.\n */\" src",
    "clean": "mkdir -p dist; cd dist; ls | grep -v 'node_modules\\|package-lock.json' | xargs rm -rf; cd ../;",
    "build": "yarn build:prep && yarn build:assets && NODE_ENV=production yarn build:sources",
    "build:prep": "yarn run clean && yarn create-indexes && cp package.json ../../README.md dist",
    "build:assets": "sync-folders --type copy webpack service-worker docs dist",
    "build:sources": "babel ./src --source-maps --out-dir dist",
    "watch": "yarn build:prep && concurrently 'yarn build:assets --watch' 'yarn build:sources --watch'",
    "docs": "mkdir -p docs/build && PRESERVE_MARKDOWN=true NODE_ENV=docs babel-node docs/buildDocs d.js",
    "link": "yarn build; cd dist; npm link; yarn link; cd ..;"
  },
  "peerDependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.2",
    "babel-plugin-react-storefront": "^6.0.0",
    "file-loader": "^2.0.0",
    "lodash": "^4.17.11",
    "mobx": "^4.9.4",
    "mobx-react": "^5.4.3",
    "mobx-state-tree": "^3.14.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.3.5",
    "react-svg-loader": "^2.1.0",
    "react-transition-group": "^2.5.0",
    "react-universal-component": "3.0.3",
    "source-map-loader": "^0.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "chai": "^4.1.2",
    "create-index": "^2.3.0",
    "documentation": "moovweb/documentation#feature/preserve-markdown",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "gh-pages": "^1.2.0",
    "globby": "^8.0.1",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^1.5.0",
    "jsdom": "^12.0.0",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.1",
    "nock": "^10.0.6",
    "prettier": "^1.16.4",
    "react-docgen": "^2.20.1",
    "regenerator-runtime": "^0.11.1",
    "simulant": "^0.2.2",
    "sinon": "^5.0.3",
    "storybook-addon-jsx": "^5.4.0",
    "sync-folders": "^2.0.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^9.0.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.6",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "cookie": "^0.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "delegate": "^3.2.0",
    "eslint": "^5.9.0",
    "eslint-config-moov": "^0.0.3",
